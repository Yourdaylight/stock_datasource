{
  "table_name": "ods_fina_indicator",
  "table_type": "ods",
  "columns": [
    {"name": "ts_code", "data_type": "LowCardinality(String)", "nullable": false, "comment": "TS股票代码"},
    {"name": "ann_date", "data_type": "Date", "nullable": true, "comment": "公告日期"},
    {"name": "end_date", "data_type": "Date", "nullable": false, "comment": "报告期"},
    {"name": "roe", "data_type": "Nullable(Float64)", "nullable": true, "comment": "净资产收益率"},
    {"name": "roa", "data_type": "Nullable(Float64)", "nullable": true, "comment": "总资产报酬率"},
    {"name": "gross_profit_margin", "data_type": "Nullable(Float64)", "nullable": true, "comment": "销售毛利率"},
    {"name": "net_profit_margin", "data_type": "Nullable(Float64)", "nullable": true, "comment": "销售净利率"},
    {"name": "eps", "data_type": "Nullable(Float64)", "nullable": true, "comment": "每股收益"},
    {"name": "bps", "data_type": "Nullable(Float64)", "nullable": true, "comment": "每股净资产"},
    {"name": "total_revenue", "data_type": "Nullable(Float64)", "nullable": true, "comment": "营业总收入"},
    {"name": "revenue", "data_type": "Nullable(Float64)", "nullable": true, "comment": "营业收入"},
    {"name": "net_profit", "data_type": "Nullable(Float64)", "nullable": true, "comment": "净利润"},
    {"name": "total_assets", "data_type": "Nullable(Float64)", "nullable": true, "comment": "总资产"},
    {"name": "total_liab", "data_type": "Nullable(Float64)", "nullable": true, "comment": "总负债"},
    {"name": "total_equity", "data_type": "Nullable(Float64)", "nullable": true, "comment": "股东权益合计"},
    {"name": "current_ratio", "data_type": "Nullable(Float64)", "nullable": true, "comment": "流动比率"},
    {"name": "quick_ratio", "data_type": "Nullable(Float64)", "nullable": true, "comment": "速动比率"},
    {"name": "debt_to_assets", "data_type": "Nullable(Float64)", "nullable": true, "comment": "资产负债率"},
    {"name": "debt_to_equity", "data_type": "Nullable(Float64)", "nullable": true, "comment": "产权比率"},
    {"name": "asset_turnover", "data_type": "Nullable(Float64)", "nullable": true, "comment": "总资产周转率"},
    {"name": "inventory_turnover", "data_type": "Nullable(Float64)", "nullable": true, "comment": "存货周转率"},
    {"name": "receivable_turnover", "data_type": "Nullable(Float64)", "nullable": true, "comment": "应收账款周转率"},
    {"name": "version", "data_type": "UInt32", "nullable": false, "default": "toUInt32(toUnixTimestamp(now()))", "comment": "版本号用于幂等"},
    {"name": "_ingested_at", "data_type": "DateTime", "nullable": false, "default": "now()", "comment": "数据入库时间"}
  ],
  "partition_by": "toYYYYMM(end_date)",
  "order_by": ["ts_code", "end_date"],
  "engine": "ReplacingMergeTree",
  "engine_params": ["version"],
  "comment": "Financial indicators data from TuShare API"
}