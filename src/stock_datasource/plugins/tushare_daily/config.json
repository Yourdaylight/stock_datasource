{
  "enabled": true,
  "rate_limit": 120,
  "timeout": 30,
  "retry_attempts": 3,
  "description": "TuShare daily stock price data plugin",
  "schedule": {
    "frequency": "daily",
    "time": "18:00"
  },
  "parameters": {
    "max_empty_days": 5,
    "validate_prices": true,
    "require_ohlc": true
  },
  "parameters_schema": {
    "trade_date": {
      "type": "string",
      "format": "date",
      "required": true,
      "description": "Trade date in YYYYMMDD format"
    }
  }
}
