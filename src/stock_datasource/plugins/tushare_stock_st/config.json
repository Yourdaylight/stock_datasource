{
  "enabled": true,
  "rate_limit": 120,
  "timeout": 30,
  "retry_attempts": 3,
  "description": "TuShare ST股票列表数据，获取历史上每天的ST股票列表",
  "schedule": {
    "frequency": "daily",
    "time": "09:30"
  },
  "parameters_schema": {
    "trade_date": {
      "type": "string",
      "format": "date",
      "required": false,
      "description": "交易日期，格式YYYYMMDD"
    },
    "ts_code": {
      "type": "string",
      "required": false,
      "description": "股票代码"
    },
    "start_date": {
      "type": "string",
      "format": "date",
      "required": false,
      "description": "开始日期，格式YYYYMMDD"
    },
    "end_date": {
      "type": "string",
      "format": "date",
      "required": false,
      "description": "结束日期，格式YYYYMMDD"
    }
  }
}
