{
  "enabled": true,
  "rate_limit": 120,
  "timeout": 30,
  "retry_attempts": 3,
  "description": "TuShare ETF基础信息 from etf_basic API",
  "schedule": {
    "frequency": "weekly",
    "day_of_week": "monday",
    "time": "18:00"
  },
  "parameters_schema": {
    "list_status": {
      "type": "string",
      "enum": ["L", "D", "P"],
      "default": "L",
      "description": "上市状态: L上市 D退市 P待上市"
    },
    "exchange": {
      "type": "string",
      "enum": ["SH", "SZ"],
      "required": false,
      "description": "交易所: SH上交所 SZ深交所"
    }
  }
}
