{
  "table_name": "ods_etf_index",
  "table_type": "ods",
  "columns": [
    {"name": "ts_code", "data_type": "LowCardinality(String)", "nullable": false, "comment": "指数代码"},
    {"name": "indx_name", "data_type": "String", "nullable": true, "comment": "指数全称"},
    {"name": "indx_csname", "data_type": "String", "nullable": true, "comment": "指数简称"},
    {"name": "pub_party_name", "data_type": "String", "nullable": true, "comment": "指数发布机构"},
    {"name": "pub_date", "data_type": "String", "nullable": true, "comment": "指数发布日期(YYYY-MM-DD格式)"},
    {"name": "base_date", "data_type": "String", "nullable": true, "comment": "指数基日(YYYY-MM-DD格式)"},
    {"name": "bp", "data_type": "Nullable(Float64)", "nullable": true, "comment": "指数基点(点)"},
    {"name": "adj_circle", "data_type": "String", "nullable": true, "comment": "指数成份证券调整周期"},
    {"name": "version", "data_type": "UInt32", "nullable": false, "default": "toUInt32(toUnixTimestamp(now()))", "comment": "版本号用于幂等"},
    {"name": "_ingested_at", "data_type": "DateTime", "nullable": false, "default": "now()", "comment": "数据入库时间"}
  ],
  "partition_by": "tuple()",
  "order_by": ["ts_code"],
  "engine": "ReplacingMergeTree",
  "engine_params": ["version"],
  "comment": "ETF基准指数列表 from TuShare etf_index API"
}
